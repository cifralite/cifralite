<!DOCTYPE html>
<html lang="pt-BR">

<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Cifralite</title>
    <link rel="stylesheet" href="~/css/main.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/Cifralite.Web.styles.css" asp-append-version="true" />
    <link
        href="https://fonts.googleapis.com/css2?family=Material+Symbols+Outlined:opsz,wght,FILL,GRAD@20..48,100..700,0..1,-50..200"
        rel="stylesheet" />
</head>

<body>
    <header>
    </header>
    <div class="container">
        <main role="main">
            @RenderBody()
        </main>
    </div>

    <footer>
    </footer>

    <script src="~/js/site.js" asp-append-version="true"></script>
    <script src="~/js/models/acorde.js" asp-append-version="true"></script>
    <script src="~/js/models/secao.js" asp-append-version="true"></script>
    <script src="~/js/models/musica.js" asp-append-version="true"></script>
    <script src="~/js/database.js" asp-append-version="true"></script>

    @await RenderSectionAsync("Scripts", required: false)
</body>

</html>